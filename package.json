{
	"name": "digital-nomad-app",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"reset-project": "node ./scripts/reset-project.js",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"lint": "eslint --ext .tsx --ext .ts ./",
		"lint:fix": "npm run lint -- --fix",
		"prettier:write": "npx prettier --write **/*.{ts,tsx,json}",
		"prettier:format": "prettier --check ./",
		"format": "npm run prettier:write && npm run lint:fix",
		"prepare": "husky",
		"typecheck": "tsc --noEmit",
		"lint-staged": "lint-staged",
		"generate:c": "plop --plopfile ./generators/component.mjs && npm run format",
		"generate:s": "plop --plopfile ./generators/screen.mjs && npm run format",
		"generate:h": "plop --plopfile ./generators/hook.mjs && npm run format"
	},
	"dependencies": {
		"@expo/vector-icons": "^14.1.0",
		"@react-navigation/bottom-tabs": "^7.3.10",
		"@react-navigation/elements": "^2.3.8",
		"@react-navigation/native": "^7.1.6",
		"expo": "~53.0.9",
		"expo-blur": "~14.1.4",
		"expo-constants": "~17.1.6",
		"expo-font": "~13.3.1",
		"expo-haptics": "~14.1.4",
		"expo-image": "~2.1.7",
		"expo-linking": "~7.1.5",
		"expo-router": "~5.0.6",
		"expo-splash-screen": "~0.30.8",
		"expo-status-bar": "~2.2.3",
		"expo-symbols": "~0.4.4",
		"expo-system-ui": "~5.0.7",
		"expo-web-browser": "~14.1.6",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-native": "0.79.2",
		"react-native-gesture-handler": "~2.24.0",
		"react-native-reanimated": "~3.17.4",
		"react-native-safe-area-context": "5.4.0",
		"react-native-screens": "~4.10.0",
		"react-native-web": "~0.20.0",
		"react-native-webview": "13.13.5"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@commitlint/types": "^19.8.1",
		"@tanstack/eslint-plugin-query": "^5.78.0",
		"@types/eslint-plugin-security": "^3.0.0",
		"@types/react": "~19.0.10",
		"@typescript-eslint/parser": "^8.33.0",
		"conventional-changelog-atom": "^5.0.0",
		"eslint": "^8.57.1",
		"eslint-config-expo": "~9.2.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-deprecation": "^3.0.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jest": "^28.11.1",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.4.0",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-native": "^5.0.0",
		"eslint-plugin-regexp": "^2.7.0",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-sonarjs": "^3.0.2",
		"eslint-plugin-testing-library": "^7.2.2",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^15.5.2",
		"plop": "^4.0.1",
		"prettier": "3.5.3",
		"typescript": "~5.8.3"
	},
	"private": true
}
